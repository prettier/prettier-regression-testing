  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/node": "25.0.2",
    "@typescript-eslint/eslint-plugin": "8.50.0",
    "@typescript-eslint/parser": "8.50.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unicorn": "63.0.0",
    "globals": "16.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.7.4",
    "typescript": "5.9.3",
    "typescript-eslint": "8.50.0",
    "vitest": "4.0.15"
